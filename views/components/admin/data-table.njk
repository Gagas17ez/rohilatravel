{% macro blogs(blogs) %}
    <div class="card mb-4">
        <div class="card-header">
            <i class="fas fa-table me-1"></i>
            List Blog
        </div>
        <div class="card-body">
            <table id="datatablesSimple" class="table">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Judul Blog</th>
                        <th>Kategori</th>
                        <th>Status</th>
                        <th>Dibuat pada</th>
                        <th>Dirubah pada</th>
                        <th>Aksi</th>
                    </tr>
                </thead>
                <tbody>
                    {% if blogs.length < 1 %}
                        <tr>
                            <td colspan="7" style="text-align:center">Tidak ada Data Blog</td>
                        </tr>
                    {% endif %}
                    {% for blog in blogs %}
                        <tr>
                            <td>{{ loop.index }}</td>
                            <td>{{ blog.title }}</td>
                            <td>{{ blog.Category.name }}</td>
                            {% if blog.isPublished %}
                                <td><span class="badge text-bg-primary">Published</span></td>
                            {% else %}
                                <td><span class="badge text-bg-warning">Archived</span></td>
                            {% endif %}
                            <td>{{ blog.createdAt }}</td>
                            <td>{{ blog.updatedAt }}</td>
                            <td class="text-right">
                                {% if blog.isPublished %}
                                    <a href="/admin/blog/change-status/{{ blog.id }}" class="btn btn-sm btn-warning" title="Archive Selected Blogs">
                                        <i class="fa-solid fa-archive"></i>
                                    </a>
                                {% else %}
                                    <a href="/admin/blog/change-status/{{ blog.id }}" class="btn btn-sm btn-primary" title="Publish Selected Blogs">
                                        <i class="fas fa-upload"></i>
                                    </a>
                                {% endif %}
                                <a href="/blog/{{ blog.slug }}" target="_blank" class="btn btn-sm btn-dark" title="Lihat Halaman Blog">
                                    <i class="fas fa-eye"></i>
                                </a>
                                <a href="/admin/blog/edit/{{ blog.id }}" class="btn btn-sm btn-success" title="Edit Selected Blogs">
                                    <i class="fa-solid fa-pencil"></i>
                                </a>
                                <a href="/admin/blog/delete/{{ blog.id }}" class="btn btn-sm btn-danger" name="delete" title="Delete Selected Blogs">
                                <i class="fa-solid fa-trash"></i>
                                </a>
                            </td>
                        </tr>
                    {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
{% endmacro %}

