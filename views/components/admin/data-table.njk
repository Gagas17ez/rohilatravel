{% macro itinerary(itinerary) %}
    <div class="card mb-4">
        <div class="card-header">
            <i class="fas fa-table me-1"></i>
            Itinerary Table
        </div>
        <div class="card-body">
            <table id="datatablesSimple" class="table">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Lokasi/Destinasi</th>
                        <th>Jumlah Wisatawan</th>
                        <th>Mata Uang</th>
                        <th>Budget/orang</th>
                        <th>Musim</th>
                        <th>Lama Perjalanan (Hari)</th>
                        <th>Tipe Perjalanan</th>
                        <th>Transportasi</th>
                        <th>Created At</th>
                        <th>Hasil Itinerary</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>No</th>
                        <th>Lokasi/Destinasi</th>
                        <th>Jumlah Wisatawan</th>
                        <th>Mata Uang</th>
                        <th>Budget/orang</th>
                        <th>Musim</th>
                        <th>Lama Perjalanan (Hari)</th>
                        <th>Tipe Perjalanan</th>
                        <th>Transportasi</th>
                        <th>Created At</th>
                        <th>Hasil Itinerary</th>
                    </tr>
                </tfoot>
                <tbody>
                    {% for data in itinerary %}
                        <tr>
                            <td>{{ loop.index }}</td>
                            <td>{{ data.lokasi }}</td>
                            <td>{{ data.jumlah_orang }}</td>
                            <td>{{ data.mata_uang }}</td>
                            <td>{{ data.budget }}</td>
                            <td>{{ data.musim }}</td>
                            <td>{{ data.lama_perjalanan }}</td>
                            <td>{{ data.tipe_perjalanan }}</td>
                            <td>{{ data.transportasi }}</td>
                            <td>{{ data.createdAt }}</td>
                            <td>{{ data.hasil_itinerary | safe }}</td>
                        </tr>
                    {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
{% endmacro %}

