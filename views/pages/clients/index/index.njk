{% extends "layouts/clients/masterLayout.njk" %}
{% import "components/clients/index/banner.njk" as hero %}
{% import "components/clients/index/card.njk" as card %}

{% block content %}
<main id="content" class="site-main">
    <!-- ***hero start*** -->
    <section class="home-banner-section home-banner-slider">
        {{ hero.banner("MULAI PETUALANGAN DENGAN KAMI", "Selamat datang di pintu gerbang keindahan Indonesia.
        Mulailah perjalanan tak terlupakan Anda dengan perusahaan perjalanan terpercaya kami.",
        "/clients/images/destinations/Gunung-Bromo-Jawa-Timur.jpg") }}
        {{ hero.banner("JELAJAHI TUJUAN EKSOIK", "Alami keajaiban alam, dan temukan keindahan budaya yang tak
        tertandingi. Dengan kami, setiap langkah adalah petualangan yang memikat. Siapkan diri Anda untuk memasuki dunia
        baru yang memukau!", "/clients/images/destinations/flora-san-terra.jpeg") }}
    </section>
    <!-- ***hero end*** -->

    <!-- ***pencarian start*** -->
    {% include "layouts/clients/createItenary.njk" %}
    <!-- ***pencarian end*** -->

    <!-- ***Tujuan start*** -->
    {% include "layouts/clients/destinationList.njk" %}
    <!-- ***Tujuan end*** -->

    <!-- ***Callback start*** -->
    <section class="home-callback bg-img-fullcallback" style="background-image: url(/clients/images/bg-rusli.jpg);">
        <div class="overlay"></div>
        <div class="container">
            <div class="row">
                <div class="col-lg-8 offset-lg-2 text-center">
                    <div class="callback-content">
                        <div class="video-button">
                            <a id="video-container" data-fancybox="video-gallery"
                                href="https://www.youtube.com/watch?v=2OYar8OHEOU">
                                <i class="fas fa-play"></i>
                            </a>
                        </div>
                        <h2 class="section-title">APAKAH ANDA SIAP UNTUK BEPERGIAN? INGATLAH KAMI!!</h2>
                        <p>Siap-siaplah untuk memulai perjalanan tak terlupakan melalui Jawa Timur, di mana setiap sudut
                            menjanjikan petualangan, penemuan, dan keindahan yang memukau.</p>
                        <div class="callback-btn">
                            <a href="package.html" class="round-btn">Lihat Paket</a>
                            <a href="about.html" class="outline-btn outline-btn-white">Pelajari Lebih Lanjut</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ***Callback end*** -->

    <!-- ***Counter start*** -->
    <div class="home-counter">
        {% include "layouts/clients/counter.njk" %}
    </div>
    <!-- ***Counter end*** -->

    <!-- ***Paket start*** -->
    {% include "layouts/clients/package.njk" %}
    <!-- ***Paket end*** -->

    {% if recomnet == null %}
    <section class="container">
        <div class="col-lg-8 offset-lg-2 text-sm-center">
            <div class="section-heading">
                <h2 class="section-title">TIDAK ADA PAKET PROMO PADA MINGGU INI</h2>
            </div>
        </div>
    </section>
    {% else %}
    {% include "layouts/clients/recomended.njk" %}
    {% endif %}
